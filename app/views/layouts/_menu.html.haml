%header.sf-main
  .header-content
    = link_to "Home", root_path
    .divider
    = link_to "About Starfish"
    .divider
    = link_to "Discover great projects"
    .divider
    = link_to "Start your project", new_project_path 
    .divider
    %input.search-query{:type => 'text', :placeholder => 'Search'}
    .divider
    - if user_signed_in?
      =current_user.email
      |
      = link_to "Sign out", destroy_user_session_path, :method => 'delete'
    - else
      = link_to "New Account", new_user_registration_path
      |
      = link_to "Log in", new_user_session_path
